{
  "name": "crea-public",
  "version": "2.10.2",
  "description": "The public CREA Orkest website",
  "type": "module",
  "packageManager": "npm@10.9.2",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "npm run lint:styles && npm run lint:js && npm run lint:typescript",
    "lint:js": "eslint . --fix",
    "lint:styles": "prettier . --write --cache --log-level silent --list-different",
    "lint:typescript": "tsc",
    "analyze": "ANALYZE=true npm run build && webpack-bundle-analyzer .next/stats.json",
    "prepare": "husky",
    "download-schema": "rover graph fetch crea-orkest@current > ./src/graphql/generated/schema.graphql && npm run lint:styles",
    "codegen": "graphql-codegen --require dotenv/config --config ./src/graphql/codegen.ts",
    "codegen:watch": "graphql-codegen --require dotenv/config --config ./src/graphql/codegen.ts --watch",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crea-orkest/crea-public.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crea-orkest/crea-public/issues"
  },
  "homepage": "https://github.com/crea-orkest/crea-public#readme",
  "dependencies": {
    "@react-google-maps/api": "2.20.6",
    "@urql/core": "5.1.1",
    "@urql/exchange-graphcache": "7.2.3",
    "@urql/exchange-retry": "^1.3.0",
    "classnames": "2.5.1",
    "datocms-structured-text-utils": "4.1.2",
    "graphql": "16.10.0",
    "next": "14.2.26",
    "react": "18.3.1",
    "react-datocms": "7.2.2",
    "react-dom": "18.3.1",
    "urql": "4.2.2"
  },
  "devDependencies": {
    "@apollo/rover": "0.26.3",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@graphql-codegen/cli": "5.0.5",
    "@graphql-codegen/client-preset": "4.8.0",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript": "4.1.6",
    "@graphql-codegen/typescript-operations": "4.6.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.14",
    "@types/react": "18.3.20",
    "@types/react-dom": "18.3.5",
    "@typescript-eslint/eslint-plugin": "8.29.0",
    "@typescript-eslint/parser": "8.29.0",
    "dotenv": "16.4.7",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.26",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "7.1.1",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "pretty-quick": "4.1.1",
    "sass": "1.86.1",
    "ts-jest": "29.3.1",
    "ts-node": "10.9.2",
    "turbo": "2.4.4",
    "typescript": "5.8.2",
    "webpack-bundle-analyzer": "4.10.2"
  }
}
