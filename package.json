{
  "name": "crea-public",
  "version": "2.10.1",
  "description": "The public CREA Orkest website",
  "type": "module",
  "packageManager": "npm@10.8.2",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "npm run lint:styles && npm run lint:js && npm run lint:typescript",
    "lint:js": "eslint . --fix",
    "lint:styles": "prettier . --write --cache --log-level silent --list-different",
    "lint:typescript": "tsc",
    "analyze": "ANALYZE=true npm run build && webpack-bundle-analyzer .next/stats.json",
    "prepare": "husky",
    "download-schema": "rover graph fetch crea-orkest@current > ./src/graphql/generated/schema.graphql && npm run lint:styles",
    "codegen": "graphql-codegen --require dotenv/config --config ./src/graphql/codegen.ts",
    "codegen:watch": "graphql-codegen --require dotenv/config --config ./src/graphql/codegen.ts --watch",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crea-orkest/crea-public.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crea-orkest/crea-public/issues"
  },
  "homepage": "https://github.com/crea-orkest/crea-public#readme",
  "dependencies": {
    "@react-google-maps/api": "2.19.3",
    "@urql/core": "5.0.5",
    "@urql/exchange-graphcache": "7.1.2",
    "@urql/exchange-retry": "^1.3.0",
    "classnames": "2.5.1",
    "datocms-structured-text-utils": "4.0.1",
    "graphql": "16.9.0",
    "next": "14.2.5",
    "react": "18.3.1",
    "react-datocms": "7.0.1",
    "react-dom": "18.3.1",
    "urql": "4.1.0"
  },
  "devDependencies": {
    "@apollo/rover": "0.25.0",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "4.3.3",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript": "4.0.9",
    "@graphql-codegen/typescript-operations": "4.2.3",
    "@testing-library/jest-dom": "6.4.8",
    "@testing-library/react": "16.0.0",
    "@types/jest": "29.5.12",
    "@types/node": "18.19.42",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "6.2.2",
    "husky": "9.1.4",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "pretty-quick": "4.0.0",
    "sass": "1.77.8",
    "ts-jest": "29.2.3",
    "ts-node": "10.9.2",
    "turbo": "2.0.9",
    "typescript": "5.5.4",
    "webpack-bundle-analyzer": "4.10.2"
  }
}
