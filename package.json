{
  "name": "crea-public",
  "version": "2.4.2",
  "description": "The public CREA Orkest website",
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "build:turbo": "turbo run build",
    "start": "next start",
    "lint": "npm run lint:js:turbo && npm run lint:styles:turbo && npm run lint:typescript:turbo",
    "lint:turbo": "turbo run lint",
    "lint:js": "eslint . --fix",
    "lint:js:turbo": "turbo run lint:js",
    "lint:styles": "prettier . --write --cache --loglevel silent --list-different",
    "lint:styles:turbo": "turbo run lint:styles",
    "lint:typescript": "tsc",
    "lint:typescript:turbo": "turbo run lint:typescript",
    "analyze": "ANALYZE=true npm run build && webpack-bundle-analyzer .next/stats.json",
    "prepare": "husky install",
    "download-schema": "rover graph fetch crea-orkest-p7nmyh@main > ./src/graphql/generated/schema.graphql && npm run lint:styles:turbo",
    "codegen": "graphql-codegen --require dotenv/config --config ./src/graphql/codegen.ts",
    "codegen:watch": "graphql-codegen --require dotenv/config --config ./src/graphql/codegen.ts --watch",
    "turbo": "npm run build:turbo && npm run lint:turbo && npm run test:turbo",
    "test:turbo": "turbo run test",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crea-orkest/crea-public.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crea-orkest/crea-public/issues"
  },
  "homepage": "https://github.com/crea-orkest/crea-public#readme",
  "dependencies": {
    "@react-google-maps/api": "2.19.3",
    "@urql/core": "4.2.3",
    "@urql/exchange-graphcache": "6.4.1",
    "classnames": "2.5.1",
    "datocms-structured-text-utils": "3.0.0",
    "graphql": "16.8.1",
    "next": "14.1.0",
    "react": "18.2.0",
    "react-datocms": "4.1.3",
    "react-dom": "18.2.0",
    "urql": "4.0.6"
  },
  "devDependencies": {
    "@apollo/rover": "0.22.0",
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.2",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "4.2.2",
    "@graphql-codegen/introspection": "4.0.2",
    "@graphql-codegen/typescript": "4.0.4",
    "@graphql-codegen/typescript-operations": "4.1.2",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.1",
    "@types/jest": "29.5.12",
    "@types/node": "18.19.17",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.19",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "dotenv": "16.4.4",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "6.2.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "pretty-quick": "4.0.0",
    "sass": "1.71.0",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "turbo": "1.12.4",
    "typescript": "5.3.3",
    "webpack-bundle-analyzer": "4.10.1"
  },
  "module": "esnext",
  "target": "es2022"
}
