{
  "name": "crea-public",
  "version": "0.0.1",
  "description": "The public Crea Orkest website",
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true npm run build",
    "prepare": "husky install",
    "codegen": "graphql-codegen --require dotenv/config  --config ./src/graphql/codegen.ts",
    "codegen:watch": "graphql-codegen --require dotenv/config  --config ./src/graphql/codegen.ts --watch",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crea-orkest/crea-public.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crea-orkest/crea-public/issues"
  },
  "homepage": "https://github.com/crea-orkest/crea-public#readme",
  "dependencies": {
    "@apollo/client": "^3.8.0-rc.2",
    "@apollo/experimental-nextjs-app-support": "^0.4.1",
    "graphql": "16.8.0",
    "next": "13.4.16",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "turbo": "1.10.12"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/client-preset": "4.0.1",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.5.3",
    "@types/node": "18.17.5",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "dotenv": "16.3.1",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.16",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "jest-environment-jsdom": "29.6.2",
    "pretty-quick": "3.1.3",
    "sass": "1.63.6",
    "ts-node": "^10.9.1",
    "typescript": "5.1.6",
    "webpack-bundle-analyzer": "^4.9.0"
  },
  "module": "esnext",
  "target": "es2022"
}
