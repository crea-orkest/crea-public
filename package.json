{
  "name": "crea-public",
  "version": "1.0.6",
  "description": "The public CREA Orkest website",
  "type": "module",
  "packageManager": "npm@11.5.2",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "npm run lint:styles && npm run lint:js && npm run lint:typescript",
    "lint:js": "eslint . --fix",
    "lint:styles": "prettier . --write --cache --log-level silent --list-different",
    "lint:typescript": "tsc",
    "analyze": "ANALYZE=true npm run build && webpack-bundle-analyzer .next/stats.json",
    "prepare": "husky",
    "download-schema": "rover graph fetch crea-orkest@current > ./src/graphql/generated/schema.graphql && npm run lint:styles",
    "codegen": "graphql-codegen --require dotenv/config --config ./src/graphql/codegen.ts",
    "codegen:watch": "graphql-codegen --require dotenv/config --config ./src/graphql/codegen.ts --watch",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crea-orkest/crea-public.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crea-orkest/crea-public/issues"
  },
  "homepage": "https://github.com/crea-orkest/crea-public#readme",
  "dependencies": {
    "@react-google-maps/api": "2.20.7",
    "@urql/core": "5.2.0",
    "@urql/exchange-graphcache": "7.2.4",
    "@urql/exchange-retry": "1.3.2",
    "classnames": "2.5.1",
    "datocms-structured-text-utils": "4.1.2",
    "graphql": "16.11.0",
    "next": "15.5.2",
    "react": "19.1.1",
    "react-datocms": "7.2.2",
    "react-dom": "19.1.1",
    "urql": "5.0.1"
  },
  "devDependencies": {
    "@apollo/rover": "0.35.0",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@graphql-codegen/cli": "5.0.7",
    "@graphql-codegen/client-preset": "4.8.3",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript": "4.1.6",
    "@graphql-codegen/typescript-operations": "4.6.1",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@types/jest": "30.0.0 ",
    "@types/node": "22.18.0",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "@typescript-eslint/eslint-plugin": "8.42.0",
    "@typescript-eslint/parser": "8.42.0",
    "dotenv": "17.2.2",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.32",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-testing-library": "7.6.8",
    "husky": "9.1.7",
    "jest": "30.0.5",
    "jest-environment-jsdom": "30.0.5",
    "pretty-quick": "4.2.2",
    "sass": "1.92.0",
    "ts-jest": "29.4.1",
    "ts-node": "10.9.2",
    "turbo": "2.5.6",
    "typescript": "5.9.2",
    "webpack-bundle-analyzer": "4.10.2"
  }
}
