{
  "name": "crea-public",
  "version": "3.0.0",
  "description": "The public CREA Orkest website",
  "type": "module",
  "packageManager": "npm@11.9.0",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "npm run lint:styles && npm run lint:js && npm run lint:typescript",
    "lint:js": "eslint . ",
    "lint:styles": "prettier . --write --cache --log-level silent --list-different",
    "lint:typescript": "tsc",
    "analyze": "ANALYZE=true next build --webpack && webpack-bundle-analyzer .next/stats.json",
    "prepare": "husky",
    "download-schema": "rover graph fetch crea-orkest@dev > ./src/graphql/generated/schema.graphql && npm run lint:styles",
    "codegen": "graphql-codegen --require dotenv/config --config ./src/graphql/codegen.ts",
    "codegen:watch": "graphql-codegen --require dotenv/config --config ./src/graphql/codegen.ts --watch",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crea-orkest/crea-public.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crea-orkest/crea-public/issues"
  },
  "homepage": "https://github.com/crea-orkest/crea-public#readme",
  "dependencies": {
    "@react-google-maps/api": "2.20.8",
    "@urql/core": "6.0.1",
    "@urql/exchange-graphcache": "8.1.0",
    "@urql/exchange-retry": "2.0.0",
    "classnames": "2.5.1",
    "datocms-structured-text-utils": "4.1.2",
    "graphql": "16.12.0",
    "next": "16.1.6",
    "react": "19.2.4",
    "react-datocms": "7.2.14",
    "react-dom": "19.2.4",
    "urql": "5.0.1"
  },
  "devDependencies": {
    "@apollo/rover": "0.35.0",
    "@commitlint/cli": "20.4.1",
    "@commitlint/config-conventional": "20.4.1",
    "@graphql-codegen/cli": "6.1.1",
    "@graphql-codegen/client-preset": "5.2.2",
    "@graphql-codegen/introspection": "5.0.0",
    "@graphql-codegen/typescript": "5.0.7",
    "@graphql-codegen/typescript-operations": "5.0.7",
    "@next/eslint-plugin-next": "^16.0.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@types/jest": "30.0.0 ",
    "@types/node": "24.10.11",
    "@types/react": "19.2.13",
    "@types/react-dom": "19.2.3",
    "@types/webpack-bundle-analyzer": "4.7.0",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "@typescript-eslint/parser": "8.54.0",
    "dotenv": "17.2.3",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.6",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-testing-library": "7.15.4",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "pretty-quick": "4.2.2",
    "sass": "1.97.3",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "turbo": "2.8.3",
    "typescript": "5.9.3",
    "webpack-bundle-analyzer": "5.2.0"
  }
}
